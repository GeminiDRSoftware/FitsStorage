conditions:
  - matching(pdu):
    - INSTRUME:    hrwfs

import:   [ vis-pdu, ext, radec, wcs ]

hrwfs-pdu:
  - merge: vis-pdu
  - maybe-merge: radec
  - keywords:
    - UTSTART
    - UTEND
    - TIME-OBS
    - FILTER1
    - FILTER2
    - OBSTYPE
    - CCDSIZE
    - CCDSEC

hrwfs-ext:
  - merge: ext.image
  - maybe-merge: wcs
