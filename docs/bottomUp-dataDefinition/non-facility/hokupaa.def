conditions:
  - matching(pdu):
    - INSTRUME:    Hokupaa+QUIRC

import: [ vis-pdu, radec ]

validation:
  - primary-pdu: hok-pdu
  # (Hokupaa+QUIRC produces single image FITS, but AstroData makes all
  # images look like MEF no matter what... so we include this dummy
  # extension to make the validation software happy
  - extension:   ext.image

hok-pdu:
  - merge: vis-pdu
  - merge-maybe: radec
  - keywords:
    - OBJECT
    - IMAGETYP
    - TIME-OBS
    - UT
    - FILTER
    - DATASEC
