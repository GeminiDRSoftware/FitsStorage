provides:         valid
conditions:
  - is gmos

include files:    wcs

keywords:
  - CCDSUM:
    - char:     [ '1 1', '1 2', '1 4', '2 1', '2 2', '2 4', '4 1', '4 2', '4 4' ]
  - CCDSIZE:
    - pattern:    '\[\d+:\d+,\d+:\d+]'
  - CCDSEC:
    - pattern:    '\[\d+:\d+,\d+:\d+]'
  - DATASEC:
    - pattern:    '\[\d+:\d+,\d+:\d+]'
  - DETSEC:
    - pattern:    '\[\d+:\d+,\d+:\d+]'
  - BIASSEC:
    - pattern:    '\[\d+:\d+,\d+:\d+]'
  - CCDNAME:
    - char:       not null
  - GAIN:
    - float
  - RDNOISE:
    - float
