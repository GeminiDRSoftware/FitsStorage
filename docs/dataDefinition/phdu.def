conditions:
  - not exists:
    - XTENSION
  - matching:
    - TELESCOP: Gemini-South
    - TELESCOP: Gemini-North

include files:
  - pdu/flamingos
  - pdu/flamingos2
  - pdu/gmos
  - pdu/gnirs
  - pdu/nici
  - pdu/nifs
  - pdu/niri

keywords:
  - DATE
  - DATE-OBS
  - GEMPRGID
  - OBJECT
  - OBSID
  - OBSCLASS
# Maybe we need to move OBSTYPE down?
# (some instruments use OBS_TYPE)
  - OBSTYPE
  - OBSERVER
  - RAWGEMQA
  - RAWBG
  - RAWCC
  - RAWIQ
  - RAWWV
  - RELEASE
  - SSA
