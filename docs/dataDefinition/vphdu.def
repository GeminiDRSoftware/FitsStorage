conditions:
  - is non-facility
  - not exists:
    - XTENSION
  - set-date
  - matching:
    - TELESCOP: [ Gemini-South, Gemini-North ]

include files:
  - pdu/flamingos
  - pdu/hokupaa
  - pdu/hrwfs
  - pdu/michelle
  - pdu/oscir
  - pdu/phoenix
  - pdu/trecs

keywords:
  - DATE
#  - DATE-OBS
  - OBSID
  - DATALAB
#  - FRAME
  - OBJECT
  - OBSCLASS:
    - char:   [ dayCal,  partnerCal,  acqCal,  acq,  science,  progCal ]
    - since 2011-1-6
  - RELEASE:
    - date:     2000-1-1 .. *
  - IAA:
    - since 2011-1-6
  - RAWGEMQA:
    - optional
    - char:   [ UNKNOWN, USABLE, BAD, CHECK ]
  - RAWPIREQ:
    - optional
    - char:   [ UNKNOWN, 'YES', 'NO', CHECK ]   # Using quotes to prevent YAML to turn this into booleans
  - RAWBG,RAWCC,RAWIQ,RAWWV:
    - optional
    - char:     Any
    - upper:    UNKNOWN
    - pattern:  '\d{2}-percentile'

tests:
# Checks GEMPRGID, OBSID, AND DATALAB
  - valid-observation-info
  - wcs-after-pdu
