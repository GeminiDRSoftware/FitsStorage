conditions:
  - is non-facility
  - not exists:
    - XTENSION
  - set-date
  - matching:
    - TELESCOP: [ Gemini-South, Gemini-North ]

include files:
  - pdu/flamingos
  - pdu/hokupaa
  - pdu/hrwfs
  - pdu/michelle
  - pdu/oscir
  - pdu/phoenix
  - pdu/trecs

keywords:
  - DATE
  - OBSID
  - DATALAB
  - OBJECT
  - RELEASE:
    - date:     2000-1-1 .. *

  - optional:
    - RAWGEMQA:
      - char:   [ UNKNOWN, USABLE, BAD, CHECK ]
    - RAWPIREQ:
      - char:   [ UNKNOWN, 'YES', 'NO', CHECK ]   # Using quotes to prevent YAML to turn this into booleans
    - RAWBG,RAWCC,RAWIQ,RAWWV:
      - char:     Any
      - upper:    UNKNOWN
      - pattern:  '\d{2}-percentile'

  - since 2011-1-6:
    - IAA:
    - OBSCLASS:
      - char:   [ dayCal,  partnerCal,  acqCal,  acq,  science,  progCal ]

tests:
# Checks GEMPRGID, OBSID, AND DATALAB
  - valid-observation-info
  - wcs-after-pdu
