# Primary HDU for GMOS
provides:       [ valid, gmos ]
conditions:
  - matching:
    - INSTRUME: [ GMOS-N, GMOS-S ]

keywords:
  - EXPTIME:
    - float:      0 .. *
  - DARKTIME:
    - float:      0 .. *

  - MASKID:
    - int
  - MASKNAME:
    - char
  - MASKTYP:
    - int:      [ -1, 0, 1 ]
  - MASKLOC:
    - int:      [ -1, 0, 1 ]

  - FILTER1:
    - char:      not null
  - FILTID1:
    - int
  - FILTER2:
    - char:      not null
  - FILTID2:
    - int

  - GRATING:
    - char:      not null
  - GRATID:
    - int

  - FRAME
  - TIME-OBS
  - UTSTART
