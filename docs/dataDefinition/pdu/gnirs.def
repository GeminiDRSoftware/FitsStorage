# Primary HDU for GNIRS
provides:       [ valid, gnirs ]
conditions:
  - matching:
    - INSTRUME:   GNIRS

include files:    wcs

keywords:
  - ACQMIR:
    - char:     [ 'In', 'Out' ]
  - CAMERA:
    - pattern:    '.*G\d{4}'
  - COADDS:
    - int:        1 .. 999
  - DECKER:
    - pattern:    '.*G\d{4}'
  - DETBIAS:
    - float:      -5 .. 0
  - EXPTIME:
    - float:      0 .. 1000
  - FILTER1:
    - char:       'Open'
    - pattern:    '.*G\d{4}'
  - FILTER2:
    - char:       'Open'
    - pattern:    '.*G\d{4}'
  - FRAME
  - GRATING:
    - pattern:    '.*G\d{4}'
  - GRATWAVE:
    - float:      0.5 .. 6.0
  - LNRS:
    - int:        1 .. 99
  - NDAVGS:
    - int:        1 .. 99
  - PRISM:
    - pattern:    '.*G\d{4}'
  - SLIT:
    - pattern:    '.*G\d{4}'
  - TIME-OBS
  - UTSTART
