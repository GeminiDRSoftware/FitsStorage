# Primary HDU for GMOS
provides:       [ valid, gmos ]
conditions:
  - matching:
    - INSTRUME: [ GMOS-N, GMOS-S ]

keywords:
  - DARKTIME,EXPTIME:
    - float:      0 .. *

  - MASKID:
    - int
  - MASKNAME:
    - char
  - MASKTYP,MASKLOC:
    - int:      [ -1, 0, 1 ]

  - FILTER1,FILTER2:
    - char:      not null
  - FILTID1,FILTID2:
    - int

  - GRATING:
    - char:      not null
  - GRATID:
    - int

  - FRAME
  - TIME-OBS
  - UTSTART
