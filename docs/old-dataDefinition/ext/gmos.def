provides:         valid
conditions:
  - is gmos

include files:    wcs

keywords:
  - if not prepared:
    - GAIN:
      - float
    - RDNOISE:
      - float
    - CCDSUM:
      - char:     [ '1 1', '1 2', '1 4', '2 1', '2 2', '2 4', '4 1', '4 2', '4 4' ]
    - CCDNAME:
      - char:       not null
    - CCDSIZE,CCDSEC,DATASEC,DETSEC,BIASSEC:
      - pattern:    '\[\d+:\d+,\d+:\d+]'
